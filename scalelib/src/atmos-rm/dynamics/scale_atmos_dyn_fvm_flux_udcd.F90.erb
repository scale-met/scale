<%
fname ||= ENV["fname"] # for ruby <= 1.8
if /((?:ud|cd)(\d+)(?:[^\d]\w*)?)\.F90/ =~ fname
  @scheme = $1
  order = $2.to_i
  modname = "scale_atmos_dyn_fvm_flux_#{@scheme}"
  nh = (order+1)/2 - 1
  if order >= 2
    def num_diff(k="k",i="i",j="j")
                  "&
                   + GSQRT(#{k},#{i},#{j}) * num_diff(#{k},#{i},#{j})"
    end
  else
    def num_diff(*arg)
      nil
    end
  end
else
  raise "set fname="
end
class String
  def +(other)
    if Fixnum === other
      if /\A(\w+)?([+-]?\d+)?\Z/ =~ self
        off = $2.to_i + other
        if off == 0
          return $1
        else
          return "#{$1}#{off}"
        end
      end
    end
    return "#{self}#{other}"
  end
  def -(other)
    self+(-other)
  end
  def inspect
    if /\A(\w+)?[+-]?0\Z/ =~ self
      return $1
    else
      self.to_s
    end
  end
end
class Fixnum
  alias :_inspect :inspect
  def inspect
    0==self ? "" : self>0 ? "+"+_inspect : _inspect
  end
  alias :_to_s :to_s
  def to_s
    0==self ? "" : self>0 ? "+"+_to_s : _to_s
  end
end
def flux1(idx0)
  "val(#{idx0})"
end
def flux2(idx1,idx0)
  "F2 * ( val(#{idx1})+val(#{idx0}) )"
end
def flux3(idx1,idx0,idxn1)
  "( 2.0_RP * val(#{idx1}) + 5.0_RP * val(#{idx0}) - val(#{idxn1}) ) / 6.0_RP"
end
def flux3k(idx1,idx0,idxn1)
                      "( val(#{idx0}) &
                       + 0.5_RP * phi(val(#{idx1}),val(#{idx0}),val(#{idxn1})) * ( val(#{idx0})-val(#{idxn1}) ) )"
end
def flux4(idx2,idx1,idx0,idxn1)
                      "F41 * ( val(#{idx1})+val(#{idx0}) ) &
                     + F42 * ( val(#{idx2})+val(#{idxn1}) )"
end
def flux5(idx2,idx1,idx0,idxn1,idxn2)
                      "( -  3.0_RP * val(#{idx2})  &
                         + 27.0_RP * val(#{idx1})  &
                         + 47.0_RP * val(#{idx0})  &
                         - 13.0_RP * val(#{idxn1}) &
                         + 2.0_RP * val(#{idxn2}) ) / 60.0_RP"
end
def flux6(idx3,idx2,idx1,idx0,idxn1,idxn2)
                      "F61 * ( val(#{idx1})+val(#{idx0}) ) &
                     + F62 * ( val(#{idx2})+val(#{idxn1}) ) &
                     + F63 * ( val(#{idx3})+val(#{idxn2}) )"
end

def flux(vel, order, dir, k="k", i="i", j="j", dim=3, limiter=nil)
  if dim==3
    idx = [k,i,j]
  elsif dim==1
    idx = [k]
  end
  idx0 = idx.join(",")
  idx1 = idx.dup
  idx1[dir] = idx1[dir]+1
  idx1 = idx1.join(",")
  idxn1 = idx.dup
  idxn1[dir] = idxn1[dir]-1
  idxn1 = idxn1.join(",")
  idx2 = idx.dup
  idx2[dir] = idx2[dir]+2
  idx2 = idx2.join(",")
  idxn2 = idx.dup
  idxn2[dir] = idxn2[dir]-2
  idxn2 = idxn2.join(",")
  idx3 = idx.dup
  idx3[dir] = idx3[dir]+3
  idx3 = idx3.join(",")
  if Array===order && order.max % 2 == 0
    order = order.min
  end
  case order
  when Array
    ou = order[0]
    od = order[1]
    case ou
    when 1, -1
      fu = flux1(idx0)
    when 2
      fu = flux2(idx1,idx0)
    when 3
      if @scheme=="ud3Koren1993"
        fu = flux3k(idxn1,idx0,idx1)
      else
        fu = flux3(idx1,idx0,idxn1)
      end
    when 4
       fu = flux4(idx2,idx1,idx0,idxn1)
    when 5
       fu = flux5(idx2,idx1,idx0,idxn1,idxn2)
    else
       raise "invalid order: #{order}"
    end
    case od
    when 1, -1
      fd = flux1(idx1)
    when 2
      fd = flux2(idx1,idx0)
    when 3
      if @scheme=="ud3Koren1993"
        fd = flux3k(idx0,idx1,idx2)
      else
        fd = flux3(idx0,idx1,idx2)
      end
    when 4
       fd = flux4(idx2,idx1,idx0,idxn1)
    when 5
       fd = flux5(idxn1,idx0,idx1,idx2,idx3)
    else
       raise "invalid order: #{order}"
    end
    if limiter
      if limiter[0]
        fu = "#{limiter[0]} * min( 1.0_RP, &
                   ( #{fu} ) / ( #{limiter[0]} + EPS ) )"
      end
      if limiter[1]
        fd = "#{limiter[1]} * min( 1.0_RP, &
                   ( #{fd} ) / ( #{limiter[1]} + EPS ) )"
      end
    end
                 "#{fu} &
                     * ( 0.5_RP + sign(0.5_RP,#{vel}) ) &
                + #{fd} &
                     * ( 0.5_RP - sign(0.5_RP,#{vel}) )"
  when 1
    "F1 * ( val(#{idx1})+val(#{idx0}) ) - sign(F1,#{vel}) * ( val(#{idx1})-val(#{idx0}) )"
  when 2
    "#{flux2(idx1,idx0)}"
  when 3
    if @scheme=="ud3Koren1993"
                      "( val(#{idx0}) &
                       + 0.5_RP * phi(val(#{idx1}),val(#{idx0}),val(#{idxn1})) * ( val(#{idx0})-val(#{idxn1}) ) ) &
                       * ( 0.5_RP + sign(0.5_RP,#{vel}) ) &
                     + ( val(#{idx1}) &
                       + 0.5_RP * phi(val(#{idx0}),val(#{idx1}),val(#{idx2 })) * ( val(#{idx1})-val(#{idx2 }) ) ) &
                       * ( 0.5_RP - sign(0.5_RP,#{vel}) )"
    else
                      "( F31 * ( val(#{idx2})+val(#{idxn1}) ) + F32 * ( val(#{idx1})+val(#{idx0}) ) ) &
                     - ( F31 * ( val(#{idx2})-val(#{idxn1}) ) + F33 * ( val(#{idx1})-val(#{idx0}) ) ) * sign(1.0_RP,#{vel})"
    end
  when 4
                      "#{flux4(idx2,idx1,idx0,idxn1)}"
  when 5
                      "( F51 * ( val(#{idx3})+val(#{idxn2}) ) &
                       + F52 * ( val(#{idx2})+val(#{idxn1}) ) &
                       + F53 * ( val(#{idx1})+val(#{idx0}) ) ) &
                     - ( F51 * ( val(#{idx3})-val(#{idxn2}) ) &
                       + F54 * ( val(#{idx2})-val(#{idxn1}) ) &
                       + F55 * ( val(#{idx1})-val(#{idx0}) ) ) * sign(1.0_RP,#{vel})"
  when 6
                      "#{flux6(idx3,idx2,idx1,idx0,idxn1,idxn2)}"
  else
    raise "invalid order: #{order}"
  end
end

def interp(val, src, dst, k="k", i="i", j="j")
  ret = nil
  case
  when src == dst
    ret = "#{val}(#{k},#{i},#{j})"
  when src[2] == dst[2] # z==z
    ret = interpH(val, src[0..1], dst[0..1], k, i, j)
  when src[2..2] == "Z" # Z->W
    v0 = interpH(val, src[0..1], dst[0..1], k  , i, j)
    v1 = interpH(val, src[0..1], dst[0..1], k+1, i, j)
    ret = "F2H(#{k},1,I_#{dst[0..1]}Z) &
             * #{v1} &
             + F2H(#{k},2,I_#{dst[0..1]}Z) &
             * #{v0}"
  else # W=>Z
    v0 = interpH(val, src[0..1], dst[0..1], k-1, i, j)
    v1 = interpH(val, src[0..1], dst[0..1], k  , i, j)
    ret = "0.5_RP * ( #{v0} &
                        + #{v1} )"
  end
  return ret ? "( #{ret} )" : raise("BUG")
end

def interpH(val, src, dst, k="k", i="i", j="j")
  case
  when src == dst
    return "#{val}(#{k},#{i},#{j})"
  when src[0] == dst[0] # x==x, y!=y
    if src[1..1]=="Y" # Y->V
      jo = 1
    else # V->Y
      jo = -1
    end
    return "0.5_RP * ( #{val}(#{k},#{i},#{j})+#{val}(#{k},#{i},#{j+jo}) )"
  when src[1] == dst[1] # x!=x, y==y
    if src[0..0]=="X" # X->U
      io = 1
    else # U->X
      io = -1
    end
    return "0.5_RP * ( #{val}(#{k},#{i},#{j})+#{val}(#{k},#{i+io},#{j}) )"
  else # x!=x, y!=y
    if src[0..0]=="X" # X->U
      io = 1
    else # U->X
      io = -1
    end
    if src[1..1]=="Y" # Y->V
      jo = 1
    else # V->Y
      jo = -1
    end
    "0.25_RP * ( #{val}(#{k},#{i},#{j})+#{val}(#{k},#{i+io},#{j})+#{val}(#{k},#{i},#{j+jo})+#{val}(#{k},#{i+io},#{j+jo}) )"
  end
end
